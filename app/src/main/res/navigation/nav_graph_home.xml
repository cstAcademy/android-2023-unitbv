<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/fragment_recycler_view">

    <fragment
        android:id="@+id/fragment_recycler_view"
        android:name="com.cst.androidlab.fragments.FragmentRecyclerView"
        tools:layout="@layout/fragment_recycler_view">
        <action
            android:id="@+id/action_fragment_recycler_view_to_fragment_network_data"
            app:destination="@id/fragment_network_data" />
    </fragment>

    <fragment
        android:id="@+id/fragment_network_data"
        android:name="com.cst.androidlab.fragments.FragmentNetworkData"
        tools:layout="@layout/fragment_network_data"/>

</navigation>